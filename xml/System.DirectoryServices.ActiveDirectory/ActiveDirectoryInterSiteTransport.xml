    <Member MemberName="IgnoreReplicationSchedule">
      <MemberSignature Language="C#" Value="public bool IgnoreReplicationSchedule { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance bool IgnoreReplicationSchedule" />
