    <Member MemberName="AllowUnsafeCaching">
      <MemberSignature Language="C#" Value="public bool AllowUnsafeCaching { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance bool AllowUnsafeCaching" />
