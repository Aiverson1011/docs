    <Member MemberName="ViewEvent">
      <MemberSignature Language="C#" Value="public event System.Web.UI.Design.ViewEventHandler ViewEvent;" />
      <MemberSignature Language="ILAsm" Value=".event class System.Web.UI.Design.ViewEventHandler ViewEvent" />
      <MemberSignature Language="DocId" Value="E:System.Web.UI.Design.IControlDesignerView.ViewEvent" />
